    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent"/>

    <script type="text/javascript">
        function UpdateProgressBar(percentage) {
            var n = 200;
            var start = new Date().getTime();
            while (true)
                if (new Date().getTime() - start > n)
                    break;

            document.getElementById('loading-amount-div').style.width = percentage + '%';
            document.getElementById('loading-loaded-div').innerHTML = percentage + '%';
        }
    </script>
    <link rel="stylesheet" type="text/css" href="css/loading.css"/>
    <link rel="stylesheet" type="text/css" href="assets/lib/loaders/loaders.min.css">
